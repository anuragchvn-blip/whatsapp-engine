{
  "manifest_version": 3,
  "name": "WhatsApp Bulk Sender",
  "version": "1.0",
  "description": "Send bulk WhatsApp messages from your browser.",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://web.whatsapp.com/*"],
  "action": {
    "default_popup": "popup.html"
    
  },
  "background": {
    "service_worker": "background.js"
  }
}
